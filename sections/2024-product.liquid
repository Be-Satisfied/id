{%- liquid
  assign thumbs_align = section.settings.align
  assign ratio = product.media[0].preview_image.aspect_ratio
  if ratio < 1
    assign thumbs_align = 'center'
  endif
-%}

<div
  class="product-section {{ section.settings.bg }}"
  data-product-handle="{{ product.handle }}"
  data-section-id="{{ section.id }}"
  data-section-type="product"
  data-enable-video-looping="{{ section.settings.enable_video_looping }}"
  data-thumbs-align="{{ thumbs_align }}"
  {% if product.has_only_default_variant %}
    data-enable-history-state="false"
  {% else %}
    data-enable-history-state="true"
  {% endif %}
  style="--PT: {{ section.settings.padding_top }}px; --PB: {{ section.settings.padding_bottom }}px;"
>
  {%- assign current_variant = product.selected_or_first_available_variant -%}
  {%- assign image_size = '832x600' -%}
  <div class="{{ section.settings.width }} section-padding" style="max-width: 1200px;">
    <div class="product-content">
      <div class="product--content__body">
        <div class="product--content__images mobile--hide small--hide medium--show large--show widescreen--show">
          <div class="product--content__desktop-slider">
            {% for media in product.media %}
              {% case media.media_type %}
                {% when 'video' %}
                  <div class="product--content__desktop-slider-item">
                    {{ media | video_tag: class: 'media-video', image_size: image_size, controls: true }}
                  </div>
                {% else %}
                  <img
                    class="product--content__desktop-slider-item"
                    data-product-image
                    class="srcset"
                    data-image-id="{{ media.id }}"
                    alt="{{ media.alt | strip_html | escape }}"
                    aria-label="{{ media.alt | strip_html | escape }}"
                    srcset="
                      {%- if media.preview_image.width >= 550 -%}{{ media.preview_image | img_url: '550x' }} 550w,{%- endif -%}
                      {%- if media.preview_image.width >= 1100 -%}{{ media.preview_image | img_url: '1100x' }} 1100w,{%- endif -%}
                      {%- if media.preview_image.width >= 1680 -%}{{ media.preview_image | img_url: '1680x' }} 1680w,{%- endif -%}
                      {%- if media.preview_image.width >= 2048 -%}{{ media.preview_image | img_url: '2048x' }} 2048w,{%- endif -%}
                      {%- if media.preview_image.width >= 4096 -%}{{ media.preview_image | img_url: '4096x' }} 4096w{%- endif -%}
                    "
                    sizes="(min-width: 768px), 50vw, 100vw"
                    src="{{ media.preview_image | img_url: '1100x' }}"
                    loading="lazy"
                    width="100%"
                    height="auto"
                    {% if media.alt contains 'variant--' %}
                      data-variant="media.alt"
                    {% endif %}
                  >
              {% endcase %}

              <!-- 更多图片 -->
            {% endfor %}

            <button class="product--content__desktop-slider-btn left" onclick="changeSlide(-1)">&#10094;</button>
            <button class="product--content__desktop-slider-btn right" onclick="changeSlide(1)">&#10095;</button>

            <div class="product--content__desktop-slider-dot-container">
              {% for media in product.media %}
                <span class="product--content__desktop-slider-dot" onclick="currentSlide({{forloop.index0}})"></span>
              {% endfor %}
            </div>
          </div>
          <div class="product-content-policy__wrapper">
            {% comment %}
              {% if product.title == 'Deeper Connect Pico Lite' %}
                <div class="product-content-policy__flex-item">
                  <img loading="lazy" alt="" src="https://cdn.shopify.com/s/files/1/0256/1038/7504/files/B2B_Only.png?v=1716431347" />
                  <div class="product-content-policy__text">B2B Only</div>
                </div>
              {% endif %}
            {% endcomment %}

            <div class="product-content-policy__flex-item">
              <img
                loading="lazy"
                alt=""
                src="https://cdn.shopify.com/s/files/1/0256/1038/7504/files/14-day-return_icon_50bb4910-a763-4e5a-adde-79476a286b48.png?v=1706458603"
              >
              <div class="product-content-policy__text">14-Day Return</div>
            </div>
            <div class="product-content-policy__flex-item">
              <img
                loading="lazy"
                alt=""
                src="https://cdn.shopify.com/s/files/1/0256/1038/7504/files/1-year-warranty_icon_fca4c542-5e96-4426-b712-67f305bfc57d.png?v=1706458603"
              >
              <div class="product-content-policy__text">1-Year Warranty</div>
            </div>
            <div class="product-content-policy__flex-item">
              <img
                loading="lazy"
                alt=""
                src="https://cdn.shopify.com/s/files/1/0256/1038/7504/files/reliable-support-icon.png?v=1706458603"
              >
              <div class="product-content-policy__text">Reliable Support</div>
            </div>
          </div>
        </div>

        <div class="mobile--show small--one-whole medium--hide large--hide widescreen--hide">
          <div class="mobile-images-slider" style="overflow-x:scroll;">
            <div style="width:calc(87.2vw * {{ product.media.size }} + 10px * {{ product.media.size }})">
              {% for media in product.media %}
                {% case media.media_type %}
                  {% when 'image' %}
                    <div
                      class="media_item"
                      style="width: 87.2vw; float:left; border-radius: 18px; overflow:hidden;margin-right:10px"
                    >
                      <div
                        class="lazy-image"
                        style="padding-top:{{ 1 | divided_by: media.aspect_ratio | times: 100 }}%;"
                      >
                        <img
                          data-product-image
                          class="srcset"
                          data-image-id="{{ media.id }}"
                          alt="{{ media.alt | strip_html | escape }}"
                          aria-label="{{ media.alt | strip_html | escape }}"
                          srcset="
                            {%- if media.preview_image.width >= 550 -%}{{ media.preview_image | img_url: '550x' }} 550w,{%- endif -%}
                            {%- if media.preview_image.width >= 1100 -%}{{ media.preview_image | img_url: '1100x' }} 1100w,{%- endif -%}
                            {%- if media.preview_image.width >= 1680 -%}{{ media.preview_image | img_url: '1680x' }} 1680w,{%- endif -%}
                            {%- if media.preview_image.width >= 2048 -%}{{ media.preview_image | img_url: '2048x' }} 2048w,{%- endif -%}
                            {%- if media.preview_image.width >= 4096 -%}{{ media.preview_image | img_url: '4096x' }} 4096w{%- endif -%}
                          "
                          sizes="
                            (min-width: 768px), 50vw,
                            100vw
                          "
                          src="{{ media.preview_image | img_url: '1100x' }}"
                          loading="lazy"
                          width="1180"
                          height="{{ 1180 | divided_by: media.preview_image.aspect_ratio | ceil }}"
                          {% if media.alt contains 'variant--' %}
                            data-variant="media.alt"
                          {% endif %}
                        >
                      </div>
                    </div>
                  {% when 'video' %}
                    <div
                      class="media__contain"
                      style="width: 87.2vw; height:270px; float:left; border-radius: 18px; overflow:hidden;margin-right:10px;background-color:#000"
                    >
                      {{ media | video_tag: class: 'media-video', image_size: '374x270', controls: true }}
                    </div>
                {% endcase %}
              {% endfor %}
            </div>
          </div>
          <div class="policy-tips">
            {% if product.title == 'Deeper Connect Pico Lite' %}
              <div class="policy-tips-items">
                <img
                  loading="lazy"
                  alt=""
                  src="https://cdn.shopify.com/s/files/1/0256/1038/7504/files/B2B_Only.png?v=1716431347"

                >B2B Only
              </div>
            {% endif %}
            <div class="policy-tips-items">
              <img
                loading="lazy"
                alt=""
                src="https://cdn.shopify.com/s/files/1/0256/1038/7504/files/14-day-return_icon_50bb4910-a763-4e5a-adde-79476a286b48.png?v=1706458603"

              >14-Day Return
            </div>
            <div class="policy-tips-items">
              <img
                loading="lazy"
                alt=""
                src="https://cdn.shopify.com/s/files/1/0256/1038/7504/files/1-year-warranty_icon_fca4c542-5e96-4426-b712-67f305bfc57d.png?v=1706458603"

              >1-Year Warranty
            </div>
            <div class="policy-tips-items">
              <img
                loading="lazy"
                alt=""
                src="https://cdn.shopify.com/s/files/1/0256/1038/7504/files/reliable-support-icon.png?v=1706458603"

              >reliable Support
            </div>
          </div>
        </div>

        <div class="product--content__info">
          <div class="product__form__wrapper">
            {% for block in section.blocks %}
              {% case block.type %}
                {%- when '@app' -%}
                  {% render block %}
                {% when 'liquid' %}
                  <div {{ block.shopify_attributes }}>{{ block.settings.custom_liquid }}</div>
                {% when 'title' %}
                  {% render 'product-title-price', product: product, block: block, section: section %}
                {% when 'form' %}
                  <div class="product__details">
                    {% render 'product-form', product: product, block: block, section: section %}
                  </div>
                {% when 'description' %}
                  <div class="product-description rte rte--column accordion-neighbor" {{ block.shopify_attributes }}>
                    {%- assign product_description_content = product.description | strip -%}
                    {%- assign truncate_string = 'products.general.read_more' | t -%}
                    {%- assign truncate_html = '<button type="button" class="p underline" data-truncated-button>'
                      | append: truncate_string
                      | append: '</button>'
                    -%}

                    {%- if block.settings.style == 'none' -%}
                      {% if block.settings.read_more %}
                        <div
                          class="product-description__holder product-description__holder--truncate"
                          data-truncated-holder
                        >
                          <div class="product-description__inner">
                            <div
                              class="product-description__content"
                              data-truncated-content="product-description__content--truncate"
                            >
                              {{ product_description_content }}
                            </div>
                          </div>

                          <div class="product-description__actions">
                            {{ truncate_html }}
                          </div>
                        </div>
                      {% else %}
                        {{ product.description }}
                      {% endif %}
                    {% else %}
                      <div class="tabs-wrapper">
                        {% render 'product-tabs',
                          section: section,
                          block: block,
                          product: product,
                          product_description_content: product_description_content,
                          truncate_html: truncate_html
                        %}
                      </div>
                    {%- endif -%}
                  </div>
                {% when 'sharing' %}
                  {% render 'share', product: product, label: block.settings.share_label, block: block %}
                {% when 'pickup' %}
                  <div
                    class="product__pickup accordion-neighbor"
                    data-store-availability-container="{{ current_variant.id }}"
                    {{ block.shopify_attributes }}
                  ></div>
                {% when 'text' %}
                  <div
                    class="product__subheading product__subheading--rte small--hide mobile--hide"
                    {{ block.shopify_attributes }}
                  >
                    {% if block.settings.title != '' %}
                      {{ block.settings.title }}
                    {% endif %}
                  </div>

                {% when 'upsell' %}
                  <div {{ block.shopify_attributes }}>
                    {%- assign upsell_product = all_products[block.settings.product] -%}
                    {% render 'new-upsell-product', upsell_product: upsell_product, block: block, section: section %}
                    {% comment %} {% render 'air-product' %} {% endcomment %}
                  </div>
                {% when 'icon' %}
                  <div class="product__text-with-icon rte" {{ block.shopify_attributes }}>
                    <div class="icon-holder" style="--stroke: {{ block.settings.icon_color }};">
                      {%- render 'icons', filename: block.settings.icon_name -%}
                    </div>

                    <p>{{ block.settings.text | remove: '<p>' | remove: '</p>' }}</p>
                  </div>
                {% when 'image' %}
                  <div class="product__image {{ block.settings.alignment }}" {{ block.shopify_attributes }}>
                    <div
                      class="product__image__inner"
                      style="max-width: {{ block.settings.image_max_limit | append: 'px' }};"
                    >
                      {%- if block.settings.image != blank -%}
                        {%- render 'image-fit', img_object: block.settings.image -%}
                      {%- else -%}
                        {%- render 'image-fill', img_object: '' -%}
                      {%- endif -%}
                    </div>
                  </div>
              {% endcase %}
            {% endfor %}

            {% assign excluded_titles = 'Deeper Connect Wi-Fi Adapter (USB-A）,Deeper Connect Wi-Fi Adapter (USB-C）,Deeper Connect Dual Antennas Wi-Fi Adapter' %}
            {% unless excluded_titles contains product.title %}
              <div class="card-container">
                <div class="card-header" onclick="showModal('description')">
                  <div class="card-text">Description</div>
                  <div class="card-header-icon">
                    <div class="card-icon">
                      <img src="{{ "card-icon__right_arrow.png" | asset_url }}">
                    </div>
                  </div>
                </div>
                <div class="card-header" onclick="showModal('scenarios')">
                  <div class="card-text">User Scenarios</div>
                  <div class="card-header-icon">
                    <div class="card-icon">
                      <img src="{{ "card-icon__right_arrow.png" | asset_url }}">
                    </div>
                  </div>
                </div>
                <div class="card-header" onclick="showModal('estimated-delivery')">
                  <div class="card-text">Estimated Delivery</div>
                  <div class="card-header-icon">
                    <div class="card-icon">
                      <img src="{{ "card-icon__right_arrow.png" | asset_url }}">
                    </div>
                  </div>
                </div>
                <div class="card-header" onclick="showModal('specification')">
                  <div class="card-text">Specification</div>
                  <div class="card-header-icon">
                    <div class="card-icon">
                      <img src="{{ "card-icon__right_arrow.png" | asset_url }}">
                    </div>
                  </div>
                </div>
                <div class="card-header" onclick="showModal('return-policy')">
                  <div class="card-text">Warranty & Return Policy</div>
                  <div class="card-header-icon">
                    <div class="card-icon">
                      <img src="{{ "card-icon__right_arrow.png" | asset_url }}">
                    </div>
                  </div>
                </div>
              </div>
            {% endunless %}
          </div>
        </div>
      </div>
    </div>
  </div>

